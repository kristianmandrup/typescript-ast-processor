{
  "name": "@tecla5/qast-core",
  "version": "0.1.0",
  "description": "QAST core API",
  "main": "dist",
  "scripts": {
    "test": "jest",
    "typedoc:generate": "gulp typedoc",
    "typedoc:display": "open docs/api/index.html",
    "lerna:bootstrap": "lerna bootstrap --scope @tecla5/qast-core",
    "lerna:clean":
      "rm -f package.json.lock 2> /dev/null && echo y | lerna clean --scope @tecla5/qast-core"
  },
  "keywords": ["typescript", "exported", "refs"],
  "repository": {
    "type": "git",
    "url": "https://github.com/kristianmandrup/typescript-ast-traverser.git"
  },
  "author": "Kristian Mandrup <kmandrup@gmail.com>",
  "jest": {
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "license": "MIT",
  "dependencies": {
    "@tecla5/qast-loggable": "",
    "@tecla5/qast-node": "",
    "@tecla5/qast-collector": "",
    "@tecla5/qast-instrumentor": "",
    "@tecla5/qast-visitor": "",
    "@tecla5/qast-service": "",
    "@tecla5/qast-traverser": ""
  },
  "devDependencies": {}
}
